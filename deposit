money = int(input("сумма инвистиций:"))
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
#print(per_cent.get('ТКБ'))
deposit = [round((per_cent.get("ТКБ")/100)*money), round((per_cent.get("СКБ")/100)*money), round((per_cent.get("ВТБ")/100)*money), round((per_cent.get("СБЕР")/100)*money)]
print("deposit=", deposit)
print("Максимальная сумма, которую вы можете заработать —", max(deposit))
